{
  "name": "icejs",
  "version": "0.1.0",
  "private": true,
  "description": "icejs",
  "scripts": {
    "start": "icejs start --mode default",
    "build": "icejs build --mode prod",
    "build:dev": "icejs build --mode dev",
    "i18n": "yarn i18n:extract && yarn i18n:compile",
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile --strict",
    "lint": "yarn prettier && yarn eslint && yarn stylelint",
    "prettier": "prettier --write '@(src)/**/*.@(ts|tsx)'",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "yarn eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@lingui/react": "^3.13.2",
    "ahooks": "^3.3.8",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.11.1",
    "framer-motion": "^6.3.0",
    "js-cookie": "^3.0.1",
    "mac-scrollbar": "^0.10.0",
    "md5": "^2.3.0",
    "qrcode": "^1.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-remove-scroll": "^2.5.0",
    "react-share": "^4.4.0",
    "recoil": "^0.7.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@iceworks/spec": "^1.0.0",
    "@lingui/cli": "^3.13.2",
    "@lingui/core": "^3.13.2",
    "@lingui/macro": "^3.13.2",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/styled-components": "^5.1.24",
    "eslint": "^7.30.0",
    "ice.js": "^2.0.0",
    "stylelint": "^13.7.2",
    "vite-plugin-require": "^0.0.8"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
